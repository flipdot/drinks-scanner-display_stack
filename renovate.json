{
  "automerge": true,
  "automergeType": "branch",
  "docker-compose": {
    "fileMatch": ["(^|/)(stack|production)[^/]*\\.ya?ml$"]
  },
  "extends": [
    "config:base",
    "default:automergeBranchPush",
    "default:automergeDigest",
    "docker:enableMajor"
  ],
  "major": {
    "automerge": false
  }
}
